  },
  "keywords": [
    "chrome-extension",
    "linter",
    "kaggle"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.1.33",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.5",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  }
